{
    "name": "sspooky13/yaml-standards",
    "type": "library",
    "description": "YAML standards checker",
    "keywords": ["yaml", "standards", "indent", "alphabetical", "inline", "checker"],
    "homepage": "https://github.com/sspooky13/yaml-standards",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Turek",
            "email": "peter.turek13@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YamlStandards\\": [
                "src"
            ]
        },
        "classmap": [
            "src"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YamlStandards\\": [
                "tests"
            ]
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": "~5.6 || ~7.0",
        "symfony/console": "~3.0 || ~4.0.10",
        "symfony/yaml": "~3.0 || ~4.0.10",
        "sebastian/diff": "~1.4 || ~2.0 || ~3.0"
    },
    "require-dev": {
        "phing/phing": "2.16.*",
        "phpunit/phpunit": "5.7.* || 6.4.*",
        "shopsys/coding-standards": "3.1.*"
    },
    "bin": [
        "bin/yaml-standards"
    ]
}
