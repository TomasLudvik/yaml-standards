{
    "name": "sspooky13/yaml-standards",
    "type": "library",
    "description": "Standards for yaml files ",
    "keywords": ["yaml", "standards", "indent", "alphabetical", "inline", "checker", "yaml-standards", "fixer"],
    "homepage": "https://github.com/sspooky13/yaml-standards",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Turek",
            "email": "peter.turek13@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YamlStandards\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YamlStandards\\": [
                "tests"
            ]
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/console": "^3.0 || ^4.0.10",
        "symfony/config": "^3.0 || ^4.0.10",
        "symfony/yaml": "^3.0 || ^4.0.10",
        "sebastian/diff": "^1.4 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phing/phing": "^2.16",
        "phpstan/phpstan": "^0.11",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "phpunit/phpunit": "^7.5",
        "shopsys/coding-standards": "^7.3",
        "symfony/var-dumper": "^3.4 || ^4.3",
        "symplify/easy-coding-standard": "^5.4.3"
    },
    "bin": [
        "bin/yaml-standards"
    ]
}
