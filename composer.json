{
    "name": "sspooky13/yaml-standards",
    "type": "library",
    "description": "Standards for yaml files ",
    "keywords": ["yaml", "standards", "indent", "alphabetical", "inline", "checker", "yaml-standards", "fixer"],
    "homepage": "https://github.com/sspooky13/yaml-standards",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Turek",
            "email": "peter.turek13@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YamlStandards\\": [
                "src"
            ]
        },
        "classmap": [
            "src"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YamlStandards\\": [
                "tests"
            ]
        },
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "^3.0 || ^4.0.10",
        "symfony/yaml": "^3.3 || ^4.0.10",
        "sebastian/diff": "^1.4 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phing/phing": "2.16.*",
        "phpunit/phpunit": "^5.7.27 || 6.4.*",
        "shopsys/coding-standards": "3.1.*",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpmd/phpmd": "^2.6",
        "symfony/var-dumper": "^3.4 || ^4.3"
    },
    "bin": [
        "bin/yaml-standards"
    ]
}
